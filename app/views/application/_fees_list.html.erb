<h3>Other Bills</h3>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">Transaction Start Date</th>
        <th scope="col">Last Payment Date</th>
        <th scope="col">Initial Amt Owed</th>
        <th scope="col">Total Amt Paid</th>
        <th scope="col">Balance Owed</th>
        <th scope="col">Billing Type</th>
      </tr>
    </thead>
    <tbody>
      <% fees.each do |fee| %>
        <tr>
          <td><%= fee['transaction_start_date'] %></td>
          <td><%= fee['last_pmt_date'] %></td>
          <td><%= fee['initial_amt_owed'] %></td>
          <td><%= fee['total_amt_paid'] %></td>
          <td><%= fee['balance_owed'] %></td>
          <td><%= fee['billing_type'] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
