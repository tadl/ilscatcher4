<% @checkouts.each do |i| %>
  $("#checkout-history").append('<%= escape_javascript(render "list_item_2", i: i) %>')
<% end %>
<% if @more_results.to_s == "true" %>
  <% next_page = @page.to_i + 1 %>
  $("#more-checkouts").html('<%= escape_javascript(render "more_checkouts", checkouts: @checkouts, page: next_page) %>')
<% else %>
  $("#more-checkouts").html('<h2>No more items.</h2>')
<% end %>
